import java.io.*;
import java.util.*;

class Data {
	HashMap<Integer, ArrayList<String>> bucket;

	public Data() {
		bucket = new HashMap<Integer, ArrayList<String>>();
	}

	public void put(String str) {
		int len = str.length();
		if (!bucket.containsKey(len)) {
			ArrayList<String> tmp = new ArrayList<String>();
			tmp.add(str);
			bucket.put(len, tmp);
		}
	}
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(System.in), 1);
			String str = br.readLine();
			System.out.println(str.length());
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}

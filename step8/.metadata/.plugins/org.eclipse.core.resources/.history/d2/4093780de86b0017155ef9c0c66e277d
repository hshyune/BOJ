import java.io.*;
import java.util.*;

class Data {

	@SuppressWarnings("unchecked")
	public static LinkedList<Integer>[] bucket = new LinkedList[] { new LinkedList<Integer>(),
			new LinkedList<Integer>(), new LinkedList<Integer>(), new LinkedList<Integer>(), new LinkedList<Integer>(),
			new LinkedList<Integer>(), new LinkedList<Integer>(), new LinkedList<Integer>(), new LinkedList<Integer>(),
			new LinkedList<Integer>() };

	public Data() {

	}

	public void put(ArrayList<Integer> data, int radix) {
		for (int num : data) {
			int key = num / radix % 10;
			bucket[key].add(num);
		}

		int index = 0;
		
		for (int i = 0; i < 10; i++) {
			System.out.println(bucket[i]);
			if (bucket[i].isEmpty())
				continue;
			for (int num : bucket[i]) {
				data.set(index++, num);
			}
		}

	}
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(System.in), 1);
			int n = Integer.parseInt(br.readLine());
			ArrayList<Integer> data = new ArrayList<Integer>();
			for (int i = 0; i < n; i++) {
				data.add(Integer.parseInt(br.readLine()));
			}
			Data d = new Data();
			d.put(data, 1);
			d.put(data, 10);
			// d.put(data, 100);
			// d.put(data, 1000);
			// d.put(data, 10000);

		} catch (IOException e) {

		}
	}

}

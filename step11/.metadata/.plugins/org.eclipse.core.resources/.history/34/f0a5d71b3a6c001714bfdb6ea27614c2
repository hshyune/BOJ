import java.io.*;

class Stack {
	char[] data;
	int top;
	int max;

	public Stack() {
		max = 50;
		data = new char[max];
		top = -1;
	}

	public void push(char c) {
		if (top == max - 1)
			return;
		else {
			data[++top] = c;
		}
	}

	public char pop() {
		if (top == -1)
			return "0";
		else {
			return data[top--];
		}
	}
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

import java.io.*;

class Stack {
	int[] data;
	int top;
	int max;

	public Stack() {
		max = 10000;
		data = new int[max];
		top = -1;
	}

	public void push(int num) {
		if (top == max - 1)
			return;
		data[++top] = num;
		System.out.println(top);
	}

	public int pop() {
		if (top == -1)
			return -1;
		return data[top--];
	}

	public int getSize() {
		return top + 1;
	}

	public int isEmpty() {
		if (top == -1)
			return 1;
		return 0;
	}

	public int getTop() {
		return data[top];
	}

	public int cmd1(String str) {
		if (str.equals("pop")) {
			return pop();
		}
		if (str.equals("size")) {
			return getSize();
		}
		if (str.equals("empty")) {
			return isEmpty();
		}
		if (str.equals("top")) {
			return getTop();
		}
		return 0;
	}

	public void cmd2(String str, String para) {
		int p = Integer.parseInt(para);
		if (str.equals("push")) {
			push(p);
		}
	}

}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(System.in), 1);
			int cmd = Integer.parseInt(br.readLine());

			String str = null;
			Stack s = new Stack();
			for (int i = 0; i < cmd; i++) {
				str = br.readLine();

				if (st.countTokens() == 1) {
					System.out.println(s.cmd1(str));
				}
				if (st.countTokens() == 2) {
					String s1[] = str.split(" ");
					s.cmd2(s1[0], s2[1]);
				}
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}

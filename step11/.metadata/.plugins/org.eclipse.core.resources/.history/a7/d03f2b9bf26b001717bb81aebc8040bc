import java.io.*;
import java.util.*;

class Stack {
	int[] data;
	int top;
	int max;

	public Stack() {
		max = 10000;
		data = new int[10000];
		top = -1;
	}

	public void push(int num) {
		if (top == max)
			return;
		data[++top] = num;
	}

	public int pop() {
		if (top == -1)
			return -1;
		return data[top--];
	}

	public int getSize() {
		return top + 1;
	}

	public int isEmpty() {
		if (top == -1)
			return 1;
		return 0;
	}

	public int cmd(String str) {
		StringTokenizer st = new StringTokenizer(str, " ");
		if (st.countTokens() == 1) {
			System.out.println("1");
		} else {
			System.out.println("2");
		}
	}
}

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(System.in), 1);
			int cmd = Integer.parseInt(br.readLine());

			String str = null;
			Stack s = new Stack();
			for (int i = 0; i < cmd; i++) {
				str = br.readLine();
				s.cmd(str);
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}

import java.io.*;
import java.util.*;

class Node {
	int num;
	Node parent;
	ArrayList<Node> child;
	int weight;

	public Node(int num) {
		this.num = num;
		this.parent = null;
		this.child = new ArrayList<Node>();
		weight = 0;
	}

	public void addChild(Node n, int w) {
		// w=weight
		for (Node node : child) {
			if (node.num == n.num)
				return;
		}
		n.weight = w;
		n.parent = this;
		child.add(n);
	}

	public void print() {
		System.out.print("Node " + num + "(");
		if (child.size() == 0) {
			System.out.println("empty)");
		}
		for (Node node : child) {
			System.out.print(node.num + "/" + node.weight + " ");
		}
		System.out.println(") parent " + parent);
	}
}


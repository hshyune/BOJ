import java.io.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(System.in), 1);
			int n = Integer.parseInt(br.readLine());
			long[] memo = new long[n + 1];
			for (int i = 0; i <= n; i++) {
				memo[i] = i * memo[i - 1];
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
